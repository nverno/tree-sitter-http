{
  "name": "tree-sitter-http",
  "version": "3.0.0",
  "description": "Treesitter parser for http files",
  "main": "bindings/node",
  "types": "bindings/node",
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rest-nvim/tree-sitter-http.git"
  },
  "keywords": [
    "treesitter",
    "parser",
    "http"
  ],
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**"
  ],
  "author": "NTBBloodbath",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rest-nvim/tree-sitter-http/issues"
  },
  "homepage": "https://github.com/rest-nvim/tree-sitter-http#readme",
  "dependencies": {
    "node-addon-api": "^7.1.0"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.1"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "devDependencies": {
    "node-gyp-build": "^4.8.2",
    "prebuildify": "^6.0.0",
    "tree-sitter-cli": "^0.23.0"
  },
  "tree-sitter": [
    {
      "scope": "source_file.http",
      "file-types": [
        "http"
      ],
      "theme": {
        "keyword": "blue",
        "error": "red",
        "comment": "grey"
      }
    }
  ]
}
